{
    "manifest_version": 3,
    "name": "Assistance",
    "version": "1.0.0", 
    "permissions": [
        "tabs",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": ["https://trade.loginandtrade.com/webtrader/*"],
            "js": [ "src/*.js", "CONFIG/*.js", "index.js" ],
            "css": ["css/main.css"]
        }
    ]
}